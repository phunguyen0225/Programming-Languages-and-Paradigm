#include <algorithm>
#include <fstream>
#include <iostream>
#include <iterator>
#include <string>
#include <vector>

using namespace std;

template <typename T>
void getSize(T container) {
    cout << container.size() << endl;
}

struct box{
    int size() const { return 12; }
};
int main(int argc, char const *argv[]) {
    getSize(string{"Hello World!"});
    getSize(box());
    return 0;
}