class FibonacciTest extends munit.FunSuite {
  test("canary") {
    assert(true)
  }

  test("fibonacci 0") {
    assertEquals(Array(1).toSeq, FibonacciIterative(0).toSeq)
  }
  
  test("fibonacci 1") {
    assertEquals(Array(1, 1).toSeq, FibonacciIterative(1).toSeq)
  }

  test("fibonacci 2") {
    assertEquals(Array(1, 1, 2).toSeq, FibonacciIterative(2).toSeq)
  }

  test("fibonacci 3") {
    assertEquals(Array(1, 1, 2, 3).toSeq, FibonacciIterative(3).toSeq)
  }

  test("fibonacci 4") {
    assertEquals(Array(1, 1, 2, 3, 5).toSeq, FibonacciIterative(4).toSeq)
  }
}
